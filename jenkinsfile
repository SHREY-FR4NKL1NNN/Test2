pipeline{
    agent any
    stages{

        stage('Checkout'){
            steps{
                git url: 'https://github.com/SHREY-FR4NKL1NNN/Test2.git', branch: 'main'
            }
        }
        stage('Build'){
            echo 'Building the application'
        }

        stage('Test'){
            echo 'Testing the application'
        }

        stage('Deploy'){
            echo 'Deploying the application'
        }
    }
}